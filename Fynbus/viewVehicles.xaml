<Window x:Class="Fynbus.viewVehicles"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="viewVehicles" Height="273.885" Width="860.773">
    <Grid>
        <ListView Name="ListVehicles" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="633" Margin="10,10,0,0">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Reg Nr." Width="109" DisplayMemberBinding="{Binding RegNumber}" />
                    <GridViewColumn Header="Bil Nr." Width="109" DisplayMemberBinding="{Binding VehicleNumber}" />
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
